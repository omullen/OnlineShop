    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^7.4.4",
    "nodemon": "^3.0.1"
  }
}
